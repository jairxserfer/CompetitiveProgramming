    <object id="5_122811589" type="text" x="-535.5849448153251" y="-5122.272797867662" r="6.672069328168488e-30" s="1.2523861281346533" class="body">
      <height>233.28</height>
      <width>-490.8720000000001</width>
      <p>
        <text><![CDATA[DFS(grafo G)]]></text>
      </p>
      <p>
        <text><![CDATA[     PARA CADA vertice u ∈ V[G] HACER]]></text>
      </p>
      <p>
        <text><![CDATA[             estado[u] ← NO_VISITADO]]></text>
      </p>
      <p>
        <text><![CDATA[             padre[u] ← NULO]]></text>
      </p>
      <p>
        <text><![CDATA[     tiempo ← 0]]></text>
      </p>
      <p>
        <text><![CDATA[     PARA CADA vertice u ∈ V[G] HACER]]></text>
      </p>
      <p>
        <text><![CDATA[             SI estado[u] = NO_VISITADO ENTONCES]]></text>
      </p>
      <p>
        <text><![CDATA[                     DFS_Visitar(u,tiempo)]]></text>
      </p>
    </object>
