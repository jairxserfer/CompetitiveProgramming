    <object id="1_122811589" type="text" x="-1097.8623341148457" y="-5449.348796654134" r="-4.648548551097984" s="0.22720666191130479" class="body">
      <height>320.76000000000005</height>
      <width>-490.1040000000001</width>
      <p>
        <text><![CDATA[DFS_Visitar(nodo u, int tiempo)]]></text>
      </p>
      <p>
        <text><![CDATA[     estado[u] ← VISITADO]]></text>
      </p>
      <p>
        <text><![CDATA[     tiempo ← tiempo + 1]]></text>
      </p>
      <p>
        <text><![CDATA[     d[u] ← tiempo]]></text>
      </p>
      <p>
        <text><![CDATA[     PARA CADA v ∈ Vecinos[u] HACER]]></text>
      </p>
      <p>
        <text><![CDATA[             SI estado[v] = NO_VISITADO ENTONCES]]></text>
      </p>
      <p>
        <text><![CDATA[                     padre[v] ← u]]></text>
      </p>
      <p>
        <text><![CDATA[                     DFS_Visitar(v,tiempo)]]></text>
      </p>
      <p>
        <text><![CDATA[     estado[u] ← TERMINADO]]></text>
      </p>
      <p>
        <text><![CDATA[     tiempo ← tiempo + 1]]></text>
      </p>
      <p>
        <text><![CDATA[     f[u] ← tiempo]]></text>
      </p>
