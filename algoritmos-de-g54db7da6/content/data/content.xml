      </layout>
    </object>
    <object id="31_30863873" type="text" x="13719.802977209867" y="-5504.159939603733" r="-3.7357126852419125e-15" s="3.9818653694705266" class="body">
      <height>554.0400000000002</height>
      <width>705.2354511968863</width>
      <p>
        <text><![CDATA[enum VertexState { White, Gray, Black };]]></text>
      </p>
      <p>
        <text><![CDATA[ ]]></text>
      </p>
      <p>
        <text><![CDATA[…]]></text>
      </p>
      <p>
        <text><![CDATA[ ]]></text>
      </p>
      <p>
        <text><![CDATA[void Graph::DFS() {]]></text>
      </p>
      <p>
        <text><![CDATA[      VertexState *state = new VertexState[vertexCount];]]></text>
      </p>
      <p>
        <text><![CDATA[      for (int i = 0; i < vertexCount; i++)]]></text>
      </p>
      <p>
        <text><![CDATA[            state[i] = White;]]></text>
      </p>
      <p>
        <text><![CDATA[      runDFS(0, state);]]></text>
      </p>
      <p>
        <text><![CDATA[      delete [] state;]]></text>
      </p>
      <p>
        <text><![CDATA[}]]></text>
      </p>
      <p>
        <text><![CDATA[ ]]></text>
      </p>
      <p>
        <text><![CDATA[void Graph::runDFS(int u, VertexState state[]) {]]></text>
      </p>
      <p>
        <text><![CDATA[      state[u] = Gray;]]></text>
      </p>
      <p>
        <text><![CDATA[      for (int v = 0; v < vertexCount; v++)]]></text>
      </p>
      <p>
        <text><![CDATA[            if (isEdge(u, v) && state[v] == White)]]></text>
      </p>
      <p>
        <text><![CDATA[                  runDFS(v, state);]]></text>
      </p>
      <p>
        <text><![CDATA[      state[u] = Black;]]></text>
      </p>
      <p>
        <text><![CDATA[}]]></text>
      </p>
      <layout>
        <layout-element role="body" parent-id="17_30863873"/>
      </layout>
